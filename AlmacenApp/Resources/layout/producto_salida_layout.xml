
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/linearSalida"
    android:gravity="center_vertical|center_horizontal"
    android:layout_gravity="center_vertical|center_horizontal"
    android:orientation="vertical">
    <android.support.v4.widget.DrawerLayout
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/drawer_layout"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:fitsSystemWindows="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:id="@+id/ll">
        </LinearLayout>
        <include
            layout="@layout/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingTop="70dp"
            android:paddingLeft="35dp"
            android:paddingRight="35dp">
            <LinearLayout
                android:id="@+id/linearmainProducListIp"
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/md_grey_200"
                android:layout_margin="0dp"
                android:gravity="center">
                <Button
                    android:text="NUEVO"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/btnNuevaDataSalida"
                    android:drawableTop="@drawable/checklistred40"
                    android:elevation="6dp"
                    android:background="@android:drawable/editbox_background"
                    android:textColor="@color/md_black_1000"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:gravity="center"
                    android:layout_gravity="center"/>
                <Button
                    android:text="LIMPIAR"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/btnResetSalida"
                    android:background="@android:drawable/editbox_background"
                    android:drawableTop="@drawable/twoarrow40"
                    android:textColor="@color/md_black_1000"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp" />
                <Button
                    android:text="REGULARIZAR"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/btnRegularizaSalida"
                    android:background="@android:drawable/editbox_background"
                    android:drawableTop="@drawable/uploaddata40"
                    android:textColor="@color/md_black_1000"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp" />
                <TextView
                    android:text="ALMACEN: "
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/textViewIp"
                    android:layout_marginRight="10dp"
                    android:textColor="@color/md_black_1000"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_marginLeft="15dp"
                    android:textSize="18sp" />
                <Spinner
                    android:layout_width="350dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/spAlmacenSimpleSIp"
                    android:layout_gravity="center"
                    style="@android:style/Widget.Spinner"
                    android:gravity="center"
                    android:layout_marginTop="14dp"
                    android:layout_marginBottom="14dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSearchPersona"
                android:background="@android:color/white"
                android:paddingLeft="25dp"
                android:paddingRight="25dp"
                android:layout_marginBottom="2dp">
                <EditText
                    android:imeOptions="actionSearch"
                    android:inputType="text"
                    android:id="@+id/edt_searchPersonaList"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:maxLines="1"
                    android:textSize="18sp"
                    android:hint="@string/hint_search"
                    android:textColor="@android:color/black"
                    android:textColorHint="@android:color/black"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:enabled="true" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:weightSum="1">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.3"
                    android:gravity="center_vertical|center_horizontal">
                    <TextView
                        android:text="DOCUMENTO"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#343434"
                        android:layout_marginTop="10dp"
                        android:typeface="normal"
                        android:textStyle="bold"
                        android:textSize="14dp" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.7"
                    android:gravity="center_vertical|center_horizontal">
                    <TextView
                        android:text="NOMBRES"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#343434"
                        android:layout_marginTop="10dp"
                        android:typeface="normal"
                        android:textStyle="bold"
                        android:textSize="14dp" />
                </LinearLayout>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:layout_margin="0dp">
                <ListView
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/rvpersonalSalida"
                    android:background="@color/md_grey_200" />
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@android:color/white"
                android:weightSum="1"
                android:layout_marginTop="10dp">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.05"
                    android:minWidth="20px"
                    android:minHeight="25px"
                    android:layout_gravity="center"
                    android:gravity="center_vertical|center_horizontal"/>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.3"
                    android:gravity="center_vertical|center_horizontal">
                    <TextView
                        android:text="PERSONAL"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#343434"
                        android:layout_marginTop="10dp"
                        android:typeface="normal"
                        android:textStyle="bold"
                        android:textSize="13dp" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.3"
                    android:gravity="center_vertical|center_horizontal">
                    <TextView
                        android:text="PRODUCTO"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#343434"
                        android:layout_marginTop="10dp"
                        android:typeface="normal"
                        android:textStyle="bold"
                        android:textSize="13dp" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.1"
                    android:layout_gravity="center"
                    android:gravity="center_vertical|center_horizontal">
                    <TextView
                        android:text="U. M."
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#343434"
                        android:layout_marginTop="10dp"
                        android:typeface="normal"
                        android:textStyle="bold"
                        android:textSize="13dp" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.1"
                    android:gravity="center_vertical|center_horizontal">
                    <TextView
                        android:text="CANT"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#343434"
                        android:layout_marginTop="10dp"
                        android:typeface="normal"
                        android:textStyle="bold"
                        android:textSize="13dp" />
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="fill_parent"
                    android:layout_weight="0.15"
                    android:gravity="center_vertical|center_horizontal">
                    <TextView
                        android:text="ALMACEN"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#343434"
                        android:layout_marginTop="10dp"
                        android:typeface="normal"
                        android:textStyle="bold"
                        android:textSize="13dp" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:layout_marginBottom="20dp">
                <ListView
                    android:minWidth="25px"
                    android:minHeight="25px"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/rvpersonalMovimientos"
                    android:background="@color/md_grey_200" />
            </LinearLayout>
        </LinearLayout>
        <android.support.design.widget.NavigationView
            android:id="@+id/nav_view"
            android:layout_height="match_parent"
            android:layout_width="300dp"
            android:layout_gravity="start"
            android:fitsSystemWindows="true"
            app:headerLayout="@layout/nav_header" />
    </android.support.v4.widget.DrawerLayout>

</LinearLayout >